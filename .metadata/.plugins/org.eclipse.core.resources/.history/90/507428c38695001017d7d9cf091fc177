package PrimerioProjeto;

import java.util.Scanner;

public class Array {

	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		
		int[] idade = new int [5]; //vetor inteiro de 5 posições
		idade[0] = 18;
		idade [1] = 27;
		idade [2] = 13;
		idade [3] = 50;
		idade [4] = 40;
		
		System.out.println(idade);

	}
}
